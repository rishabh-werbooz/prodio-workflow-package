#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/rishabh-gupta/Werbooz/code/Insihts.com/prodio-dashboard/node_modules/.pnpm/lightningcss-cli@1.29.1/node_modules/lightningcss-cli/node_modules:/Users/rishabh-gupta/Werbooz/code/Insihts.com/prodio-dashboard/node_modules/.pnpm/lightningcss-cli@1.29.1/node_modules:/Users/rishabh-gupta/Werbooz/code/Insihts.com/prodio-dashboard/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/rishabh-gupta/Werbooz/code/Insihts.com/prodio-dashboard/node_modules/.pnpm/lightningcss-cli@1.29.1/node_modules/lightningcss-cli/node_modules:/Users/rishabh-gupta/Werbooz/code/Insihts.com/prodio-dashboard/node_modules/.pnpm/lightningcss-cli@1.29.1/node_modules:/Users/rishabh-gupta/Werbooz/code/Insihts.com/prodio-dashboard/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
"$basedir/../lightningcss-cli/lightningcss"   "$@"
exit $?
